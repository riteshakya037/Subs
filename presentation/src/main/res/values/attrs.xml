<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomSpinnerView">
        <attr name="android:entries"/>
        <attr format="enum" name="styleType">
            <enum name="default_type" value="0"/>
            <enum name="title_type" value="1"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="MaskEditText">
        <attr format="string" name="maskText"/>
        <attr format="string" name="hint"/>
        <attr name="android:inputType"/>
    </declare-styleable>
    <declare-styleable name="ExpenseView">
        <attr name="android:text"/>
        <attr name="android:background" />
    </declare-styleable>
</resources>